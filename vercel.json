{
	"version": 2,
	"builds": [
		{
			"src": "api/server.js",
			"use": "@vercel/static-build"
		},
		{
			"src": "client/dist/index.html",
			"use": "@vercel/static-build"
		}
	],
	"routes": [
		{
			"src": "/api/*",
			"dest": "api/server.js"
		},
		{
			"src": "/*",
			"dest": "client/dist/index.html"
		}
	]
}
